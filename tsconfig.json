{
    "$schema": "https://json.schemastore.org/tsconfig",
    "_version": "20.1.0",
  
    "compilerOptions": {
      "lib": ["ES2015"],
      "module": "ESNext",
      "target": "ESNext",
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "moduleResolution": "bundler"
    },
    "include": ["index.ts", "types.ts"]
  }